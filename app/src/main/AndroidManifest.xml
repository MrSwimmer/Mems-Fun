<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.membattle">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".Application"
        android:allowBackup="true"
        android:icon="@drawable/logo_circlel"
        android:label="@string/app_name"
        android:roundIcon="@drawable/bez"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".intro.Splash"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".sign_in_up.RegActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".sign_in_up.LoginActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".intro.FirstStartActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".game.PlayActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".main_activity.MainActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".TestMenu.TestMenu">
        </activity>
    </application>

</manifest>